{
  "compilerOptions": {
    "target": "es5",
    "module": "es6",
    // "lib": [],
    "jsx": "preserve",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    // "outFile": "./",
    "outDir": "./client/www",
    "rootDir": "./client/src",
    "downlevelIteration": true,

    "strict": true,
    // "noImplicitAny": true,
    // "strictNullChecks": true,
    // "strictFunctionTypes": true,
    // "strictPropertyInitialization": true,
    // "noImplicitThis": true,
    // "alwaysStrict": true,

    // "noUnusedLocals": true,
    // "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "baseUrl": "client/src",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  }
}
